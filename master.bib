	Doi = {10.1086/171192},
	Journal = {Astrophysical Journal},
	Keywords = {DENSITY WAVE MODEL, HARMONIC OSCILLATION, HYDRODYNAMICS, INTERSTELLAR GAS, LANDAU DAMPING, POISSON EQUATION, RESONANCE, SPIRAL GALAXIES, STELLAR MOTIONS,GRAVITATIONAL FIELDS},
