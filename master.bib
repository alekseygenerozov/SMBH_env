	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data\_query?bibcode=2008ApJ...672...83M\&link\_type=ABSTRACT},
	Volume = {672},
	Year = {2008},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data%5C_query?bibcode=2008ApJ...672...83M%5C&link%5C_type=ABSTRACT},
