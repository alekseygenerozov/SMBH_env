	Keywords = {accretion discs, black hole physics, gravitational waves, methods: numerical,accretion},
	Month = aug,
	Pages = {3033--3041},
	Primaryclass = {astro-ph.CO},
	Title = {{Limiting eccentricity of subparsec massive black hole binaries surrounded by self-gravitating gas discs}},
