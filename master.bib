	Title = {{Tracing Black Hole Mergers Through Radio Lobe Morphology}},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data\_query?bibcode=2002Sci...297.1310M\&link\_type=ABSTRACT},
