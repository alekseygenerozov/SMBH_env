	Journal = {\mnras},
	Keywords = {Cosmology: Theory, accretion, accretion discs, galaxies: active, galaxies: formation,black hole physics},
	Month = apr,
	Pages = {1591},
	Title = {{Retrograde accretion and merging supermassive black holes}},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data\_query?bibcode=2011MNRAS.412.1591N\&link\_type=ABSTRACT},
	Volume = {412},
	Year = {2011},
	Bdsk-Url-1 = {http://adsabs.harvard.edu/cgi-bin/nph-data%5C_query?bibcode=2011MNRAS.412.1591N%5C&link%5C_type=ABSTRACT},
