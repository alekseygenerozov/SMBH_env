	Arxivurl = {http://arXiv.org/abs/astro-ph/0602549},
	Author = {{Allen}, S.~W. and {Dunn}, R.~J.~H. and {Fabian}, A.~C. and {Taylor}, G.~B. and {Reynolds}, C.~S.},
