	Doi = {10.1063/1.2943607},
	Journal = {ORIGIN OF MATTER AND EVOLUTION OF GALAXIES: The 10th International Symposium on Origin of Matter and Evolution of Galaxies: From the Dawn of Universe to the Formation of Solar System. AIP Conference Proceedings},
