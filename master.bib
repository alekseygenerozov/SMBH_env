	Month = apr,
	Pages = {L93},
	Title = {{The Afterglow of Massive Black Hole Coalescence}},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data\_query?bibcode=2005ApJ...622L..93M\&link\_type=ABSTRACT},
	Volume = {622},
