@article{NixonKing+:2011a,
	Abstract = {We consider the interaction between a binary system (e.g. two supermassive black holes or two stars) and an external accretion disc with misaligned angular momentum. This situation occurs in galaxy merger events involving supermassive black holes, and in the formation of stellar--mass binaries in star clusters. We work out the gravitational torque between the binary and disc, and show that their angular momenta J\_b, J\_d stably counteralign if their initial orientation is sufficiently retrograde, specifically if the angle theta between them obeys cos(theta) < -J\_d/2J\_b, on a time short compared with the mass gain time of the central accretor(s). The magnitude J\_b remains unchanged in this process. Counteralignment can promote the rapid merger of supermassive black hole binaries, and possibly the formation of coplanar but retrograde planets around stars in binary systems.},
	Annote = {Accepted in MNRAS Letters. 4 pages, 1 figure},
	Author = {Nixon, Chris and King, Andrew and Pringle, Jim},
	Date-Modified = {2014-07-14 23:51:45 +0000},
	Journal = {eprint arXiv},
	Keywords = {Astrophysics - Cosmology and Extragalactic Astrop, Astrophysics - High Energy Astrophysical Phenomen,Astrophysics - Galaxy Astrophysics},
	Month = jul,
	Pages = {5056},
	Title = {{The final parsec problem: aligning a binary with an external accretion disc}},
	Url = {http://adsabs.harvard.edu/cgi-bin/nph-data\_query?bibcode=2011arXiv1107.5056N\&link\_type=ABSTRACT},
